server {
    listen         8080 default_server;
    server_name localhost;
    root           .;
    index          index.html;
    try_files $uri /index.html;
    location /akdssd {
        laosasda;
        adads;
        as;
    }
    location / {
        root .;
        adads;
        as;
    }
    location /aads {
        root merdols;
        laosasda;
        adads;
        as;
    }
}
