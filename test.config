server {
    listen         8082;
    server_name    lolserv;
    location / {
        allow_methods GET;
    }
    location / .cpp{
        e_404             error_pages/404.html;
        e_405             error_pages/405.html;
        root              src;
        cgi_return        cgi_lol.sh;
    }
}

