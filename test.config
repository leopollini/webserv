server {
    listen         8084;
    server_name    localhost:8082 www.lol.com;
    root           .;
    index          index.html;
    try_files $uri /index.html;
    e_404             404.html;
    e_403             error_pages/403.html;
    autoindex       yes;
    location /haha {
        root haha/;
        index   file.html;
    }
    location /akdssd {
        return            307   http://google.com/~;
    }
    location /nothere .cpp {
        root .;
       # return            302 /src/~;
    }
    location / .cpp{
        e_404             error_pages/404.html;
        e_405             error_pages/405.html;
        #root              src;
        cgi_return        cgi_lol.sh;
    }
    location / .hpp{
        #allow_methods     POST;
        e_405             error_pages/405.html;
        e_404             error_pages/404.html;
        root              include;
        return            307   akdssd;
    }
    location / .html{
        #root        html;
        e_404             error_pages/404.html;
    }
    location / {
    autoindex yes;
        e_404             error_pages/404.html;
        root        .;
    }
    location /html {
        e_404             error_pages/404.html;
        index          file.html;
        root        html;
    }
    location /aads {
        root .;
    }
}

