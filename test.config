server {
    listen 1234;
    location /{
        allow_methods GET;
    }
}

server {
    listen         8081 def _oos;
    server_name    localhost;
    root           .;
    index          index.html;
    try_files $uri /index.html;
    e_404             404.html;
    e_403             error_pages/403.html;
    location /akdssd {
        laosasda;
        adads;
        as;
    }
    location / .cpp{
        e_404             error_pages/404.html;
        e_405             error_pages/405.html;
        return     301    localhost:8081;
    }
    location / .hpp{
        allow_methods     POST;
        e_405             error_pages/405.html;
        e_404             error_pages/404.html;
        root             include;
        adads;
        as;
    }
    location / .html{
        root        html;
        e_404             error_pages/404.html;
        adads;
        as;
    }
    location / {
        e_404             error_pages/404.html;
        root        .;
        adads;
        as;
    }
    location /html {
        e_404             error_pages/404.html;
    index          file.html;
        root        html;
        adads;
        as;
    }
    location /aads {
        root merdols;
        laosasda;
        adads;
        as;
    }
}