server {
    listen         8080 default_server;
    server_name    localhost;
    root           .;
    index          index.html;
    try_files $uri /index.html;
    e_404             404.html;
    e_403             error_pages/403.html;
    location /akdssd {
        laosasda;
        adads;
        as;
    }
    location / .cpp{
        e_404             error_pages/404.html;
        e_405             error_pages/405.html;
        allow_methods     GET POST HEAD;
        root        src;
        adads;
        as;
    }
    location /src .cpp{
        e_404             error_pages/404.html;
        e_405             error_pages/405.html;
        allow_methods     GET POST HEAD;
        root        src;
        adads;
        as;
    }
    location / .hpp{
        e_404             error_pages/404.html;
        root        include;
        adads;
        as;
    }
    location / .html{
        e_404             error_pages/404.html;
        root        html;
        adads;
        as;
    }
    location / {
        e_404             error_pages/404.html;
        root        .;
        adads;
        as;
    }
    location /html {
        e_404             error_pages/404.html;
    index          file.html;
        root        html;
        adads;
        as;
    }
    location /aads {
        root merdols;
        laosasda;
        adads;
        as;
    }
}